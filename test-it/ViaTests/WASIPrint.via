define(MyVI dv(VirtualInstrument (
    Locals: c(
        e(c(
            e(.Boolean status)
            e(.Int32 code)
            e(.String source)
        ) error)
        e(.Occurrence occurrence)
    )
    clump (
        Printf("Before JS Invoke\n")
        JavaScriptInvoke(occurrence false error 'WASI_PRINT' *)
        Printf("After JS Invoke\n")
    )
) ) )

enqueue(MyVI)
